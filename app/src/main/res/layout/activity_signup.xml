<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent">

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <RelativeLayout android:id="@+id/page__1_ek1"
                android:layout_marginTop="0dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <View   android:id="@+id/_bg___1_ek2"
                    android:background="@drawable/_bg___1_ek2_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="415dp"
                    android:layout_height="926dp" />

                <View   android:id="@+id/rectangle_31"
                    android:background="@drawable/white_round_corner"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="236dp"
                    android:layout_width="411dp"
                    android:layout_height="690dp" />

                <TextView android:id="@+id/_login_ek1"
                    android:textSize="20sp"
                    android:textColor="@color/type_here____color"
                    android:layout_marginLeft="101dp"
                    android:layout_marginTop="260dp"
                    android:layout_width="83dp"
                    android:layout_height="37dp"
                    android:text="@string/_login_ek1_string"/>

                <TextView android:id="@+id/sign_up_ek1"
                    android:textSize="20sp"
                    android:textColor="@color/_forgotten_password__color"
                    android:layout_marginLeft="260dp"
                    android:layout_marginTop="260dp"
                    android:layout_width="102dp"
                    android:layout_height="37dp"
                    android:text="@string/sign_up_ek1_string"/>


                    <ImageView
                        android:id="@+id/ride2"
                        android:layout_marginStart="90dp"
                        android:layout_width="210dp"
                        android:layout_height="210dp"
                        android:layout_marginTop="5dp"
                        android:src="@drawable/main_logo" />


                <View   android:id="@+id/rectangle_33_ek1"
                    android:background="@drawable/rectangle_33_ek1_shape"
                    android:layout_marginStart="214dp"
                    android:layout_marginTop="306dp"
                    android:layout_width="168dp"
                    android:layout_height="2dp"/>

                <RelativeLayout android:id="@+id/group_36"
                    android:layout_marginTop="336dp"
                    android:layout_marginStart="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <RelativeLayout android:id="@+id/group_30_ek1"
                        android:layout_marginTop="99dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <EditText  android:id="@+id/txtphone"
                            android:background="@drawable/rectangle_2_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="25dp"
                            android:hint="Type here..."
                            android:paddingLeft="18dp"
                            android:textSize="15sp"
                            android:textColorHint="@color/type_here____color"
                            android:layout_width="349dp"
                            android:layout_height="51dp" />

                        <TextView android:id="@+id/phone_number"
                            android:textSize="16sp"
                            android:textColor="@color/_forgotten_password__color"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="133dp"
                            android:layout_height="30dp"
                            android:text="@string/phone_number_string"/>

                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/group_34"
                        android:layout_marginTop="198dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <EditText  android:id="@+id/txtspassword"
                            android:background="@drawable/rectangle_2_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="25dp"
                            android:hint="Type here..."
                            android:paddingLeft="18dp"
                            android:textSize="15sp"
                            android:drawableRight="@drawable/ic_sharp_visibility_off_24"
                            android:paddingRight="10dp"
                            android:textColorHint="@color/type_here____color"
                            android:layout_width="349dp"
                            android:layout_height="51dp" />

<!--                        <ImageView android:id="@+id/hide_ek1"-->
<!--                            android:src="@drawable/hide"-->
<!--                            android:contentDescription="hide_ek1"-->
<!--                            android:layout_marginStart="307dp"-->
<!--                            android:layout_marginTop="40dp"-->
<!--                            android:layout_width="20dp"-->
<!--                            android:layout_height="20dp"/>-->

                        <TextView android:id="@+id/password_ek1"
                            android:textSize="16sp"
                            android:textColor="@color/_forgotten_password__color"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="96dp"
                            android:layout_height="30dp"
                            android:text="@string/password_ek1_string"/>

                    </RelativeLayout>
                    <RelativeLayout android:id="@+id/group_35"
                        android:layout_marginTop="297dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <EditText  android:id="@+id/txtcpassword"
                            android:background="@drawable/rectangle_2_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="25dp"
                            android:hint="Type here..."
                            android:paddingLeft="18dp"
                            android:drawableRight="@drawable/ic_sharp_visibility_off_24"
                            android:paddingRight="10dp"
                            android:textSize="15sp"
                            android:textColorHint="@color/type_here____color"
                            android:layout_width="349dp"
                            android:layout_height="51dp" />

                        <TextView android:id="@+id/confirm_password"
                            android:textSize="16sp"
                            android:textColor="@color/_forgotten_password__color"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="161dp"
                            android:layout_height="30dp"
                            android:text="@string/confirm_password_string"/>
                    </RelativeLayout>

                    <RelativeLayout android:id="@+id/group_31_ek1"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <EditText  android:id="@+id/txtsuname"
                            android:background="@drawable/rectangle_2_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="25dp"
                            android:hint="Type here..."
                            android:paddingLeft="18dp"
                            android:textSize="15sp"
                            android:textColorHint="@color/type_here____color"
                            android:layout_width="349dp"
                            android:layout_height="51dp" />

                        <ImageView android:id="@+id/polygon_1_ek1"
                            android:src="@drawable/polygon_1"
                            android:contentDescription="polygon_1_ek1"
                            android:layout_marginStart="315dp"
                            android:layout_marginTop="45dp"
                            android:layout_width="13.07dp"
                            android:layout_height="10.62dp"/>

                        <TextView android:id="@+id/username"
                            android:textSize="16sp"
                            android:textColor="@color/_forgotten_password__color"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="100dp"
                            android:layout_height="30dp"
                            android:text="@string/username_string"/>

                    </RelativeLayout>

                    <Button
                        android:id="@+id/btnsignup"
                        android:background="@drawable/rectangle_15_shape"
                        android:layout_marginTop="398dp"
                        android:layout_marginStart="88dp"
                        android:textSize="20dp"
                        android:layout_width="160dp"
                        android:text="Sign Up"
                        android:layout_height="50dp" />
                </RelativeLayout>

            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>


